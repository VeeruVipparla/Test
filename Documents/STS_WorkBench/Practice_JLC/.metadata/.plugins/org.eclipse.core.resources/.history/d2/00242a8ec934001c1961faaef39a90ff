package com.interview.practice;

public class Recursive {

	public static void main(String[] args) {
		String str = "Veeru";
		System.out.println(recursiveMethod(str));

	}

	static String recursiveMethod(String str) {
		if ((null == str) || (str.length() <= 1)) {
			return str;

		}
		return null;
	}

}
