package com.snr.demo.test;

public class Array {

	public static void main(String[] args) {
		int arr[][]=null;
		arr=new int[3][];
		int tmp[]= {99,88,77,66,55};
		System.out.println(("Length:"+arr.length));
		arr[0]=new int[4];
				arr[1]=new int[] {10,20,30};
				arr[3]=tmp;
				for(int ar[]:arr) {
					for(int a:ar)
						System.out.println("\t"+a);
					System.out.println();
		
	
		}

	}

}
